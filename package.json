  "dependencies": {
    "circular-json": "^0.5.9",
    "express": "^4.16.4",
    "fuzzysort": "^1.1.4",
    "install": "^0.12.2",
    "lodash.isstring": "^4.0.1",
    "node-tesseract-ocr": "^0.1.0",
    "npm": "^6.6.0",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
